#nothing

<SimulationCanvas>:
    chainA:chainA
    chainB:chainB
    lineT:lineT
    frameLeft:frameLeft
    frameTop:frameTop
    frameRight:frameRight
    frameBottom:frameBottom
    scatterInstance:scatterInstance
    xPosSlider:xPosSlider
    yPosSlider:yPosSlider
    angleA:angleA
    angleB:angleB
    angleP:angleP
    sled:sled
    lengthToXY:lengthToXY
    posToLength:posToLength
    
    
    ScatterPlane:
        do_rotation: False
        id: scatterInstance
        Label: 
            id: scatterObject
            text: ""
        SimulationLine:
            id: chainA
        SimulationLine:
            id: chainB
        SimulationLine:
            id: lineT
        SimulationLine:
            id: frameLeft
        SimulationLine:
            id: frameTop
        SimulationLine:
            id: frameRight
        SimulationLine:
            id: frameBottom
        SimulationAngle:
            id: angleA
        SimulationAngle:
            id: angleB
        SimulationAngle:
            id: angleP
        SimulationSled:
            id: sled
        PosToChainLength:
            id: posToLength
        ChainLengthtoXY:
            id: lengthToXY
    Slider:
        id: xPosSlider
        size_hint_x: .25
        pos: 0,-root.height/2 + 25
        min:-100
        max:root.bedWidth+100
    Slider:
        id: yPosSlider
        size_hint_x: .25
        pos: 400,-root.height/2 + 25
        min:-100
        max:root.bedHeight+100
        
<SimulationAngle>:
    Label:
        text: root.angleAsString
        font_size: 35
        pos: root.textPos

<SimulationLine>:
    canvas:
        Color:
            rgb: root.color
        Line:
            points: root.fromPos[0], root.fromPos[1],root.toPos[0], root.toPos[1]
    Label:
        text: root.lengthAsString
        font_size: 35
        pos: -root.width/2 + (root.fromPos[0] + root.toPos[0])/2, -root.height/2 + (root.fromPos[1] + root.toPos[1])/2

<ChainLengthtoXY>:
    Label:
        text: root.xVal
        font_size:35
        pos:2400,100
    Label:
        text: root.yVal
        font_size:35
        pos:2400,50

<PosToChainLength>:
    Label:
        text: root.lenAString
        font_size:35
        pos:2400,250
    Label:
        text: root.lenBString
        font_size:35
        pos:2400,300
    
<SimulationSled>:
    canvas:
        Color:
            rgb: (1,0,0)
        #Line:
        #    points: root.sledToolPos[0]+.000000001*root.sledPointTwo[1], root.sledToolPos[1], root.sledPointOne[0],root.sledPointOne[1]
        #    width: 6
        #Line:
        #    points: root.sledToolPos[0]+.000000001*root.sledPointTwo[1], root.sledToolPos[1],root.sledPointTwo[0],root.sledPointTwo[1]
        #    width: 6
        Line:
            points: root.sledPointOne[0], root.sledPointOne[1], root.sledPointTwo[0], root.sledPointTwo[1]
            width: 6
        Line:
            points: root.sledToolPos[0]+.000000001*root.sledPointTwo[1], root.sledToolPos[1], root.sledMidpointTop[0], root.sledMidpointTop[1]
            width: 1
        Line:
            points: root.sledPointOne[0], root.sledPointOne[1], root.bottomRight[0], root.bottomRight[1]
            width: 6
        Line:
            points: root.sledPointTwo[0], root.sledPointTwo[1], root.bottomLeft[0], root.bottomLeft[1]
            width: 6
        Line:
            points: root.bottomLeft[0]+.000000001*root.sledPointTwo[1], root.bottomLeft[1], root.bottomRight[0], root.bottomRight[1]
            width: 6
    Label:
        text: root.slantAsString
        font_size:35
        pos:-100,50
    Label:
        text: root.toolPosAsString
        font_size:35
        pos:-100,0
    Label:
        text: root.topLengthAsString
        font_size:35
        pos:-100,-50
    Label:
        text: root.correctionFactorString
        font_size:35
        pos:-100,-100
    Label:
        text: root.errorDistString
        font_size:35
        pos:-100,-150